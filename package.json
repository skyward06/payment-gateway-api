  "scripts": {
    "seed": "prisma db seed",
    "prisma:studio": "prisma studio -n 0.0.0.0",
    "dev": "ts-node-dev --no-notify -r tsconfig-paths/register --respawn --transpile-only src/index.ts",
    "start": "node dist/src/index.js",
    "build": "tsc && tsc-alias",
    "prod": "ts-node -r tsconfig-paths/register src/index.ts",
    "start:dev": "NODE_ENV=development pm2 start ecosystem.config.js",
    "start:staging": "NODE_ENV=staging pm2 start ecosystem.config.js",
    "start:prod": "NODE_ENV=production pm2 start ecosystem.config.js",
    "start:cluster": "pm2 start ecosystem.config.js",
    "stop": "NODE_ENV=${NODE_ENV:-development} pm2 stop ecosystem.config.js",
    "stop:dev": "NODE_ENV=development pm2 stop ecosystem.config.js",
    "stop:staging": "NODE_ENV=staging pm2 stop ecosystem.config.js",
    "stop:prod": "NODE_ENV=production pm2 stop ecosystem.config.js",
    "restart": "NODE_ENV=${NODE_ENV:-development} pm2 restart ecosystem.config.js",
    "restart:dev": "NODE_ENV=development pm2 restart ecosystem.config.js",
    "restart:staging": "NODE_ENV=staging pm2 restart ecosystem.config.js",
    "restart:prod": "NODE_ENV=production pm2 restart ecosystem.config.js",
    "reload": "NODE_ENV=${NODE_ENV:-development} pm2 reload ecosystem.config.js",
    "reload:dev": "NODE_ENV=development pm2 reload ecosystem.config.js",
    "reload:staging": "NODE_ENV=staging pm2 reload ecosystem.config.js",
    "reload:prod": "NODE_ENV=production pm2 reload ecosystem.config.js",
    "delete": "NODE_ENV=${NODE_ENV:-development} pm2 delete ecosystem.config.js",
    "delete:dev": "NODE_ENV=development pm2 delete ecosystem.config.js",
    "delete:staging": "NODE_ENV=staging pm2 delete ecosystem.config.js",
    "delete:prod": "NODE_ENV=production pm2 delete ecosystem.config.js"
